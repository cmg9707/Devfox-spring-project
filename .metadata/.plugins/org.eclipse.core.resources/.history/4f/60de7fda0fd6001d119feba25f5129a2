<mapper namespace="com.DevFox.JUS.mapper.JUSMapper">
	<select id="User_Count" resultType="int">
		select count(*) from user
	</select>
	
	<select id="User_Chk" resultType="int">
		select count(*) from user where user_id = #{user_id_value}
	</select>
</mapper>                       
                   